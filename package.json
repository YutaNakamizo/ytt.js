  },
  "homepage": "https://github.com/YutaNakamizo/ytt.js#readme",
  "devDependencies": {
    "@types/jest": "^26.0.23",
    "jest": "^27.0.4",
    "ts-jest": "^27.0.2",
    "ts-loader": "^9.2.2",
    "typescript": "^4.3.2"
  },
  "dependencies": {
    "@types/node": "^15.12.1"
